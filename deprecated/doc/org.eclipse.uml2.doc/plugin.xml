<plugin
   id="org.eclipse.uml2.doc"
   name="%pluginName"
   version="1.0.1"
   provider-name="%providerName">

   <requires>
      <import plugin="org.eclipse.core.runtime.compatibility"/>
      <import plugin="org.eclipse.help"/>
   </requires>

<!-- ====================================================================== -->
<!-- Define primary TOC                                                     -->
<!-- ====================================================================== -->
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>

</plugin>
